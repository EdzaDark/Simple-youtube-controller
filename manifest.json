{

    "description": "Simple Youtube player controller",
    "manifest_version": 2,
    "name": "Youtube controller",
    "version": "1.25",
    "permissions":[
        
        "tabs"
    ],
    "content_scripts": [
        {
            "matches":[
            "*://www.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    
    "browser_action": {
      "browser_style": true,
      "default_popup": "popup/choose_page.html",
      "default_icon": {
       
        "32": "icon/logo.png"
      }
    }
  
  }